{
  "name": "your-component",
  "version": "1.1.0",
  "author": "your-name",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "typings": "types/index.d.ts",
  "files": [
    "lib",
    "types",
    "src/components"
  ],
  "scripts": {
    "version": "npm run build",
    "postversion": "git push origin master && npm publish",
    "dev": "vite",
    "build": "cross-env NODE_ENV=production rollupx && eslint --fix lib/**/*.d.ts --no-ignore",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "start": "npm run dev",
    "test": "vitest --environment jsdom",
    "docs:dev": "cd docs && npm run docs:dev",
    "docs:build": "cd docs && npm run docs:build"
  },
  "dependencies": {
    "@babel/runtime": "^7.x",
    "core-js": "^3.x",
    "element-ui": "^2.x",
    "style-inject": "^0.x",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-runtime-helpers": "^1.x"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.45",
    "@vitejs/plugin-vue2": "^1.1.2",
    "@vue/composition-api": "^1.7.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^1.3.0",
    "@vue/tsconfig": "^0.1.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.22.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^9.0.0",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rollupx": "^2.x",
    "sass": "^1.54.0",
    "typescript": "~4.7.4",
    "vite": "^3.0.1",
    "vitest": "^0.18.1",
    "vue-template-compiler": "^2.7.10",
    "vue-tsc": "^0.38.8"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "bugs": {
    "url": "https://github.com/your-name/your-component/issues"
  },
  "homepage": "https://github.com/your-name/your-component#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/your-name/your-component.git"
  }
}
